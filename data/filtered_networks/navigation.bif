  (Acceptable, Inaccurate) 0.6, 0.3, 0.1;
  (Inaccurate, Inaccurate) 0.03, 0.07, 0.90;
}
probability ( ToShowAircraftPositionBasedOnGPSSignal | ToAcquireGPSsignal ) {
  (Accurate) 0.95, 0.04, 0.01;
  (Acceptable) 0.04, 0.95, 0.01;
  (Inaccurate) 0.01, 0.04, 0.95;
