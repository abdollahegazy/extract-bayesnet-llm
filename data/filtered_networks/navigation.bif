network unknown {
}
variable ToAcquireGPSsignal {
  type discrete [ 3 ] { Accurate, Acceptable, Inaccurate };
}
variable ToCheckAircraftPositionExecutingRNPProcedure {
  type discrete [ 3 ] { Accurate, Acceptable, Inaccurate };
}
variable ToKeepAircraftOnProgrammedRoute {
  type discrete [ 3 ] { Accurate, Acceptable, Inaccurate };
}
variable ToShowAircraftPositionBasedOnGPSSignal {
  type discrete [ 3 ] { Accurate, Acceptable, Inaccurate };
}
probability ( ToAcquireGPSsignal ) {
  table 0.85, 0.10, 0.05;
}
