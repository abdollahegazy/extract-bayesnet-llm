network unknown {
}
variable AverageDistance {
  type discrete [ 3 ] { 0-300, 300-600, >600 };
}
variable BlueConcrete {
  type discrete [ 2 ] { False, True };
}
variable FloorArea {
  type discrete [ 5 ] { 0-150, 150-220, 220-360, 360-1500, >1500 };
}
probability ( AverageDistance | BlueConcrete ) {
  (False) 0.10, 0.04, 0.86;
  (True) 0.39, 0.53, 0.08;
}
probability ( BlueConcrete ) {
  table 0.46, 0.54;
}
probability ( FloorArea | AverageDistance ) {
  (0-300) 0.05, 0.04, 0.19, 0.31, 0.41;
  (300-600) 0.25, 0.06, 0.19, 0.26, 0.24;
  (>600) 0.26, 0.39, 0.21, 0.09, 0.05;
}
